{
  "name": "book-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "yarn lint-staged",
    "pre-commit": "yarn lint-staged",
    "lint:eslint": "eslint --ignore-pattern \"submodules/**\" \"./**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-next": "^13.0.5",
    "lint-staged": "^13.0.3",
    "next": "13.0.5",
    "prettier": "^2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "7.0.0",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-prettier": "^2.0.0",
    "validate-branch-name": "^1.3.0"
  },
  "lint-staged": {
    "**/*.{css,scss}": [
      "stylelint --ignore-path .gitignore '**/*.{css,scss}' --fix",
      "prettier --ignore-path .gitignore '**/*.{css,scss}' --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --ignore-path .gitignore '**/*.{js,jsx,ts,tsx}' --fix",
      "prettier --ignore-path .gitignore '**/*.{js,jsx,ts,tsx}' --write"
    ]
  }
}
